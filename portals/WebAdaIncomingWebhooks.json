            "320c3fd5-8ab0-4c4a-a0a8-6c2b26e2a270": {
                "externalScripts": [
                    "https://use.fontawesome.com/releases/v5.0.10/js/all.js"
                ],
                "id": "320c3fd5-8ab0-4c4a-a0a8-6c2b26e2a270",
                "props": {
                    "html": {
                        "dataType": "DYNAMIC_DATA_TYPE",
                        "force_data": [
                            "STATIC_DATA_TYPE",
                            "DYNAMIC_DATA_TYPE"
                        ],
                        "id": "28e10deb-7033-4a40-8db6-a538dd40de0c",
                        "incoming_parser": {
                            "isDebugOn": false,
                            "value": {
                                "CSS": ".incoming-requests {\n  color: gray;\n}\n\n.incoming-requests .title {\n  font-size: 18px;\n  font-weight: bold;\n}\n\n.new-request-container {\n  padding: 10px;\n  margin: 10px;\n  border-radius: 5px;\n  background-color: lightgray;\n}\n\n.request-time {\n  float: right;\n}\n\n.selected {\n  color: white;\n  background-color: darkgray;\n}",
                                "HTML": "\u003cdiv class=\"incoming-requests\"\u003e\n  \u003cdiv class=\"title\"\u003eINCOMING REQUESTS\u003c/div\u003e\n  \u003cdiv class=\"request-list\" id=\"requestList\"\u003e\n    \n  \u003c/div\u003e\n\u003c/div\u003e\n",
                                "JavaScript": "//JavaScript\n\nif (this.datasource) {\n    var incomingRequest = JSON.parse(this.datasource);\n    var newRequestContainer = document.createElement(\"div\");\n    newRequestContainer.classList.add(\"new-request-container\");\n    newRequestContainer[\"data-request-obj\"] = this.datasource; \n    newRequestContainer.onclick = function (requestObj, evt) {\n        var requestContainer;\n        if (evt.target.classList.contains(\"new-request-container\")) {\n            requestContainer = evt.target;\n        } else {\n            requestContainer = evt.target.parentNode;\n        }\n        if (window.lastSelectedRequest) {\n            window.lastSelectedRequest.classList.remove(\"selected\");\n        }\n        requestContainer.classList.add(\"selected\");\n        CB_PORTAL.portalModel.getDatasourceInstancesWithNameAsKey().selectedRequest.sendData(requestObj);\n        window.lastSelectedRequest = requestContainer;\n    }.bind(this, incomingRequest);\n    var urlContainer = document.createElement(\"div\");\n    urlContainer.classList.add(\"request-url\");\n    urlContainer.innerText = incomingRequest.request_url.indexOf(\"?\") \u003e -1 ? incomingRequest.request_url.substring(0, incomingRequest.request_url.indexOf(\"?\")) : incomingRequest.request_url;\n    newRequestContainer.appendChild(urlContainer);\n    var timeContainer = document.createElement(\"div\");\n    timeContainer.classList.add(\"request-time\");\n    timeContainer.innerText = new Date(incomingRequest.time_received).toLocaleString();\n    newRequestContainer.appendChild(timeContainer);\n    var requestTypeContainer = document.createElement(\"div\");\n    requestTypeContainer.classList.add(\"request-type\");\n    requestTypeContainer.innerText = incomingRequest.method;\n    newRequestContainer.appendChild(requestTypeContainer);\n    var requestList = document.getElementById(\"requestList\");\n    requestList.insertBefore(newRequestContainer, requestList.firstChild);\n}"
                            }
                        }
                    }
                },
